Create package name as week1.day1 under src/main/java
package week.day1;
public class Car {
public void applyBreak() {
System.out.println("Applied break");
}
public void soundHorn() {
System.out.println("soundHorn");
}

public class Bike {
public static void main(String args[]) {
Car obj =new Car();
Bike obj1 = new Bike();
obj.applyBreak();
obj.soundHorn();
}
}
