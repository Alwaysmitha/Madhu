package Week1.day2;

public class Primenumbers {
	public static void main(String[] args)	{
		int num = 13;
		boolean flag = false;
		int a = num/2;
		for(int i=2; i<a; i++) {
			if(num%i==0)
			{
			flag = true;
			break;
					}
		}
		if(flag==true) {
			System.out.println("The given number is not Prime");
		}
		else
		{
			System.out.println("The given number is Prime");
		}
	}
		
}
